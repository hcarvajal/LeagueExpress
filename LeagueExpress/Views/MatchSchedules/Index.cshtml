@model IEnumerable<LeagueExpress.Models.MatchSchedule>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.matchWeek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchHomeTeam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchAwayTeam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchHomeResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchAwayResult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.matchStatus)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.matchWeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchHomeTeam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchAwayTeam)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchHomeResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchAwayResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchStatus)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
